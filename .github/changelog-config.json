{
  "categories": [
    {
      "title": "## 🚀 Features",
      "labels": ["feature", "enhancement"]
    },
    {
      "title": "## 🐛 Bug Fixes", 
      "labels": ["bug", "fix"]
    },
    {
      "title": "## 🔒 Security",
      "labels": ["security", "vulnerability"]
    },
    {
      "title": "## 📚 Documentation",
      "labels": ["documentation", "docs"]
    },
    {
      "title": "## 🧪 Testing",
      "labels": ["test", "testing", "ci"]
    },
    {
      "title": "## ⚡ Performance",
      "labels": ["performance", "optimization"]
    },
    {
      "title": "## 🔧 Maintenance",
      "labels": ["maintenance", "refactor", "chore"]
    },
    {
      "title": "## 📦 Dependencies",
      "labels": ["dependencies", "deps"]
    }
  ],
  "ignore_labels": [
    "ignore-for-release",
    "duplicate",
    "question",
    "invalid",
    "wontfix"
  ],
  "sort": {
    "order": "ASC",
    "on_property": "mergedAt"
  },
  "template": "${{CHANGELOG}}\n\n**Full Changelog**: ${{UNCATEGORIZED}}",
  "pr_template": "- ${{TITLE}} #${{NUMBER}} (@${{AUTHOR}})",
  "empty_template": "- No changes",
  "label_extractor": [
    {
      "pattern": "\\[(.*)\\]",
      "target": "$1"
    }
  ],
  "duplicate_filter": {
    "pattern": "\\[(.*)\\]",
    "on_property": "title",
    "method": "match"
  },
  "transformers": [
    {
      "pattern": "[\\-\\*] (.+)",
      "target": "- $1"
    }
  ],
  "max_tags_to_fetch": 100,
  "max_pull_requests": 100,
  "max_back_track_time_days": 90,
  "exclude_merge_branches": [
    "Owner/qa"
  ],
  "tag_resolver": {
    "method": "sort",
    "filter": {
      "pattern": "v\\d+\\.\\d+\\.\\d+"
    }
  }
}