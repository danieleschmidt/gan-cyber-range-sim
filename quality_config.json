{
  "monitoring": {
    "enabled": true,
    "update_interval": 30.0,
    "alert_thresholds": {
      "test_coverage": 80.0,
      "security_scan": 90.0,
      "performance_benchmark": 75.0,
      "code_quality": 85.0,
      "compliance_check": 95.0
    }
  },
  "progressive_validation": {
    "enabled": true,
    "stages": [
      "generation_1",
      "generation_2",
      "generation_3",
      "production"
    ],
    "fail_fast": false,
    "auto_fix": true
  }
}